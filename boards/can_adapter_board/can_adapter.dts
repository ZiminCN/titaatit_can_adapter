		zephyr,flash = &flash0;
		zephyr,master-canfd = &fdcan2;
		zephyr,canbus = &fdcan2;
		zephyr,slave-canfd = &fdcan3;
		zephyr,peripheral-canfd = &fdcan1;
	};

