&rtc {
	clocks = <&rcc STM32_CLOCK_BUS_APB1 0x00000400>,
		 <&rcc STM32_SRC_LSI RTC_SEL(2)>;
	status = "okay";
};

&flash0 {
	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		slot0_partition: partition@0 {
			label = "image-0";
			reg = <0x00000000 DT_SIZE_K(216)>;
		};

		factory_partition: partition@3fC00 {
			label = "factory_arg";
			reg = <0x0003FC00 DT_SIZE_K(2)>;
		};
	};
};

&iwdg {
	status = "okay";
};

&rng {
	status = "okay";
};

